{
    "name":"copy-pasty-chrome",
    "description":"A chrome extension to boost the process of copy-paste of text in web browser",
    "manifest_version":2,
    "version":"1.0",
    "content_scripts": [ {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      } ],
    
    "background":{
        "scripts":["background.js"]
    },
    "permissions":["clipboardWrite","clipboardRead"]
    


    

}